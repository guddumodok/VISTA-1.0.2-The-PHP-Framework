    $time=$no*1;
    
}else if($end=="M" OR $end=="m"){
    $time=$no*60;
    
}else if($end=="h" OR $end=="H"){
    $time=$no*3600;
}else{
    echo"<script>alert('Redirect Time Is Not Set Poperly !');</script>";
}

    echo"<meta http-equiv='refresh'content='".$time.";url=".$url."'>";

}
